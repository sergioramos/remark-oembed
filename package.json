{
  "name": "remark-oembed",
  "description": "Converts URLs surrounded by newlines into embeds that are loaded asynchronously",
  "version": "1.2.1",
  "repository": "sergioramos/remark-oembed",
  "main": "index.js",
  "keywords": [
    "oembed",
    "markdown",
    "remark",
    "remarkjs",
    "remark-plugin",
    "html-embed",
    "async",
    "defer"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.com",
    "access": "public"
  },
  "scripts": {
    "eslint": "eslint . --ext .js",
    "fmt": "prettier --config package.json --write '**/*'",
    "test": "NODE_ENV=test c8 -r lcovonly -r html -r text ava --timeout 9999 --serial"
  },
  "dependencies": {
    "apr-intercept": "^3.0.4",
    "got": "^11.7.0",
    "hast-util-select": "^4.0.0",
    "hast-util-to-html": "^7.1.1",
    "hastscript": "^6.0.0",
    "is-url": "^1.2.4",
    "mime": "^2.4.6",
    "param-case": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@mdx-js/mdx": "^1.6.18",
    "@rollup/plugin-virtual": "^2.0.3",
    "ava": "^3.13.0",
    "c8": "^7.3.4",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-xo-space": "^0.25.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "mz": "^2.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "puppeteer": "^5.3.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "rollup": "^2.31.0",
    "rollup-plugin-babel": "^4.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --config package.json --write",
      "git add"
    ],
    "*.*": [
      "prettier --config package.json --write",
      "git add"
    ]
  },
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "xo-space/esnext",
      "prettier"
    ],
    "rules": {
      "new-cap": 0,
      "camelcase": 0,
      "capitalized-comments": 0
    }
  }
}
